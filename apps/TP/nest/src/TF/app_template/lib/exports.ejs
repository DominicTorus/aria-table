<%_ const formatString = (str) => str.toLowerCase().split(' ').join('_'); _%>

// EXPORT ALL SCREENS AND WIDGETS
<%_ if (data.length > 0) { _%>
    <%_ data.forEach((screen) => { _%>
export './screens/<%= formatString(screen) %>/<%= formatString(screen) %>.dart';
    <%_ if (data1[screen] && data1[screen].length > 0) { _%>
        <%_ data1[screen].forEach((widget) => { _%>
export './screens/<%= formatString(screen) %>/widgets/<%= widget %>';
        <%_ }) _%>
    <%_ } _%>
    <%_ }) _%>
<%_ } _%>

